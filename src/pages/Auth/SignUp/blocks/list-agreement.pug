    q-item-section(side) 
      q-btn(flat size="xs" dense class="see-more-btn q-pl-sm" @click="goToTermsCondition('terms-of-service')")
        span(class="see-more-label") 자세히보기
        q-icon(name="chevron_right" size="20px")

  q-item(class="item-lists")
